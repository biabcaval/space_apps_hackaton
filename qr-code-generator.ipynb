   "source": [
    "import urllib.parse\n",
    "import qrcode\n",
    "\n",
    "number = \"558181447236\"\n",
    "message = \"Hello, here is my location: -7.1249765,-34.8695281\"\n",
    "encoded_message = urllib.parse.quote(message)\n",
    "url = f\"https://wa.me/{number}?text={encoded_message}\"\n",
    "qr = qrcode.make(url)\n",
    "qr.save(\"whatsapp_qr.png\")"
   ]
  }
